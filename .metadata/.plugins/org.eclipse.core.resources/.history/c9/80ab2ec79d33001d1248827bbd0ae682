package entrada_processamento_saida;

import java.util.Scanner;
public class Ex19 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		double p1=0, p2=0,media = (p1 + (p2 * 2)) / 3;
		
		System.out.printf("Nota da P1:");
		p1 = ler.nextInt();
		System.out.printf("Nota da P2:");
		p2 = ler.nextInt();
		media = (p1 + (p2 * 2)) / 3;
		if(media >= 5) {
			System.out.printf("Sua Media é:%f, Aprovado!",media);
		}
		else {
			System.out.printf("Sua Media é:%f, Reprovado!",media);
		}

	}

}
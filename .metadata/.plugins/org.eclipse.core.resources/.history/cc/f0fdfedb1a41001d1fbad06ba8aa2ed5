package Ex54;
import java.util.Scanner;
public class Ex54 {

	public static void main(String[] args) {
		Scanner ler=new Scanner(System.in);
		
		char cb;
		
		Cliente[] listaCliente=new Cliente[5];
		
		ContaBancaria[] listaConta=new ContaBancaria[5];
		
		for(int i=0;i<=4;i++) {
			Cliente a=new Cliente();
			ContaBancaria b=new ContaBancaria();
			
			System.out.print("");
			a.id=ler.nextInt();
			
			System.out.print("");
			a.nome=ler.next();
			
			System.out.print("");
			a.idade=ler.nextInt();
			
			System.out.print("");
			a.email=ler.next();
				
			System.out.print("");
			cb=ler.next().charAt(0);
			
			if(cb =='s') {
				System.out.print("");
				b.agencia=ler.next();
				
				System.out.print("");
				b.numero=ler.next();
				
				System.out.print("");
				b.saldo=ler.nextDouble();
			}else {
				b=new ContaBancaria(null); 
			}
		}
	}

}
